{
  "name": "bear-app-template",
  "packageManager": "pnpm@9.4.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "pnpm run build:generate-env && pnpm run build:icons && pnpm run build:remix",
    "build:generate-env": "tsx ./other/generate-env.ts",
    "build:icons": "tsx ./other/build-icons.ts",
    "build:remix": "remix vite:build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev": "pnpm run build:generate-env && pnpm run build:icons && remix vite:dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "test": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@conform-to/dom": "1.1.5",
    "@conform-to/react": "1.1.5",
    "@conform-to/zod": "1.1.5",
    "@epic-web/client-hints": "1.3.3",
    "@epic-web/invariant": "1.0.0",
    "@radix-ui/react-dialog": "1.1.0",
    "@radix-ui/react-dropdown-menu": "2.1.0",
    "@radix-ui/react-scroll-area": "1.1.0",
    "@radix-ui/react-slot": "1.1.0",
    "@remix-run/node": "2.9.2",
    "@remix-run/react": "2.9.2",
    "@remix-run/serve": "2.9.2",
    "@types/cookie": "0.6.0",
    "@vercel/analytics": "1.3.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "cookie": "0.6.0",
    "isbot": "5.1.17",
    "nanoid": "5.0.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "spin-delay": "2.0.0",
    "tailwind-merge": "2.3.0",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "0.45.0",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "8.57.0",
    "@remix-run/dev": "2.9.2",
    "@sly-cli/sly": "1.10.0",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/fs-extra": "11.0.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@vercel/remix": "2.9.2-patch.2",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-i": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-react": "7.34.3",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "53.0.0",
    "eslint-plugin-vitest": "0.5.4",
    "execa": "9.2.0",
    "fs-extra": "11.2.0",
    "glob": "10.4.5",
    "globals": "15.6.0",
    "lint-staged": "15.2.10",
    "node-html-parser": "6.1.13",
    "postcss": "8.4.41",
    "prettier": "3.3.2",
    "prettier-plugin-pkg": "0.18.1",
    "remix-flat-routes": "0.6.5",
    "simple-git-hooks": "2.11.1",
    "tailwindcss": "3.4.4",
    "ts-dedent": "2.2.0",
    "tsx": "4.15.6",
    "typescript": "5.4.5",
    "typescript-eslint": "7.13.1",
    "vite": "5.4.2",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  }
}
